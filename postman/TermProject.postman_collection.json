	"event": [
		{
			"listen": "prerequest",
			"script": {
				"type": "text/javascript",
				"packages": {},
				"requests": {},
				"exec": [
					"// baseUrl 자동 주입 (혹시 누락 방지)\r",
					"if (!pm.environment.get(\"baseUrl\")) {\r",
					"    pm.environment.set(\"baseUrl\", \"http://localhost:8080\");\r",
					"}\r",
					"\r",
					"// accessToken이 있으면 Authorization 자동 세팅\r",
					"const token = pm.environment.get(\"accessToken\");\r",
					"if (token) {\r",
					"    pm.request.headers.upsert({\r",
					"        key: \"Authorization\",\r",
					"        value: `Bearer ${token}`\r",
					"    });\r",
					"}\r",
					""
				]
			}
		},
